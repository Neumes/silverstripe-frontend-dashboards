		<div class="dashlet-title-actions">
			<a href="#" class="dashlet-action-toggle ui-icon ui-icon-minus"></a>
			<a href="#" class="dashlet-action-edit ui-icon ui-icon-wrench"></a>
			<a href="#" class="dashlet-action-delete ui-icon ui-icon-close"></a>
